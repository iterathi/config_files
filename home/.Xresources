!! Cursor fix
Xcursor.theme:Bibata-Modern-Ice
Xcursor.size:18

!! Colorscheme - Tokyo Night

*.foreground:    #ABB2BF
*.background:    #1E2127
*.cursorColor:   #5C6370
*.highlightColor:#3A3F4B

!*fading:                  40
!*fadeColor:               S_base03
!*pointerColorBackground:  S_base01
!*pointerColorForeground:  S_base1

! Normal      Bright
! --------    -------
! 0 Black      8 Black
! 1 Red        9 Red
! 2 Yellow    10 Yellow
! 3 Green     11 Green
! 4 Blue      12 Blue
! 5 Magenta   13 Magenta
! 6 Cyan      14 Cyan
! 7 White     15 White

! Black colours
*.color0:        #1E2127
*.color8:        #5C6370
! Red colours
*.color1:        #E06C75
*.color9:        #E06C75
! Yellow colours
*.color2:        #98C379
*.color10:       #98C379
! Green colours
*.color3:        #D19A66
*.color11:       #D19A66
! Blue colours
*.color4:        #61AFEF
*.color12:       #61AFEF
! Magenta (pink, !green) colours
*.color5:        #C678DD
*.color13:       #C678DD
! Cyan (blue + green)
*.color6:        #56B6C2
*.color14:       #56B6C2
! White colours
*.color7:        #ABB2BF
*.color15:       #FFFFFF


!! URxvt Appearance

URxvt.font: xft:HackNerdFontMono-Regular:style=Regular:size=12
URxvt.boldFont: xft:HackNerdFontMono-Bold:style=Bold:size=12
URxvt.italicFont: xft:HackNerdFontMono-Italic:style=Italic:size=12
URxvt.boldItalicfont: xft:HackNerdFontMono-BoldItalic:style=Bold Italic:size=12
URxvt.letterSpace: 0
URxvt.lineSpace: 2
URxvt.geometry: 115x27
URxvt.internalBorder: 12
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.iso14755: false


!! URxvt Keybinds
! scroll one line higher
URxvt.keysym.Shift-Up: command:\033]720;1\007
! scroll one line lower
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
! jump to next word
URxvt.keysym.Control-Right: \033[1;5C
! jump to previous word
URxvt.keysym.Control-Left: \033[1;5D

!! Copy Paste & Other Extensions
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1
